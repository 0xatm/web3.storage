{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "checkJs": true,
    "target": "esnext",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "preserve",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "web3.storage": ["../client"]
    }
  },
  "include": [
    "lib",
    "pages",
    "components",
    "content",
    "hooks",
    "modules",
    "OLD/upload.js",
    "OLD/tokens.js",
    "OLD/new-token.js",
    "OLD/login.js",
    "OLD/files.js",
    "OLD/callback.js",
    "OLD/about.js"
  ],
  "exclude": ["node_modules/", ".next/"],
  "references": [
    {
      "path": "../client"
    }
  ]
}
